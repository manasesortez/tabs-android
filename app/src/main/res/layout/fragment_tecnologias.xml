<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    android:layout_height="match_parent"
    tools:context=".ui.main.Tecnologias">

    <TextView
        android:id="@+id/lblEstudio"
        android:layout_width="379dp"
        android:layout_height="70dp"
        android:text="Lenguajes de Programacion:\nJava, Python, Kotlin, JS, TS"
        android:textColor="#005E8E"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        tools:ignore="HardcodedText" />


    <TextView
        android:layout_width="379dp"
        android:layout_height="70dp"
        android:text="Skills Extras: Photoshop, Premiere Pro, Adobe Audition, Lightroom CC"
        android:textColor="#005E8E"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.331"
        tools:ignore="HardcodedText" />

    <TextView
        android:layout_width="379dp"
        android:layout_height="70dp"
        android:text="Bases de Datos: MySQL, SQL Server, Mongodb, Firebase, SQLite"
        android:textColor="#005E8E"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.199"
        tools:ignore="HardcodedText" />
</androidx.constraintlayout.widget.ConstraintLayout>